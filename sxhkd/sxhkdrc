# Reload SXHKD
super + Escape
    pkill -USR1 -x sxhkd

# Volume
super + shift + d
    volumeStatus

# Emoji
super + period
    rofi -show emoji

# Wifi Rofi
super + w
    wifiRofi

# Firefox
super + shift + w
   openFirefox 

# Screenshot
super + shift + s
    screenshot

# Clipboard
super + v
    clipmenu

# Discord
super + shift + d
    discord

# Battery Info
super + b
     batteryInfo

